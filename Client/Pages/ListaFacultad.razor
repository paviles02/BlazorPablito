@page "/listaFacultad"
@using BlazorPablito.Shared
@inject HttpClient http

@inject HttpClient http



@code{
    string[] cabecera = { "ID", "Nombre" };
}

<VisorTabla micabecera="cabecera" listaDato="listaFacultad" />

@code {
    private FacultadModel[] listaFacultad;

    protected override async Task OnInitializedAsync()
    {
        listaFacultad = await http.GetFromJsonAsync<FacultadModel[]>("api/Facultad/Get");
    }




}
